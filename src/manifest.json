{
	"manifest_version": 2,
	"name": "Plauser",
	"description": "Allows you to play/pause your currently playing track from any tab.",
	"version": "0.8.1",
	"permissions": 
	[
		"tabs",
		"https://play.google.com/music/*",
		"https://play.spotify.com/*",
		"https://www.youtube.com/*",
		"http://grooveshark.com/*",
		"https://soundcloud.com/*"
	],
  	"icons": 
	{ 
		"16": "images/icon-plauser-16.png",
		"48": "images/icon-plauser-48.png",
		"128": "images/icon-plauser-128.png"
	},
	"background": 
	{
		"page": "background.html"
	},
	"browser_action": 
	{
		"default_icon": "images/icon-plauser-128.png"
	}
}
